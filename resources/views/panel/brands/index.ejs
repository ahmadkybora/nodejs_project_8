<%- include('../../layouts/panel/header'); %>
<div class="container max-auto">
    <a href="/panel/brands/create" class="btn btn-success">Register Brands</a>
    <table class="table">
        <thead class="text-center">
        <tr>
            <th>#</th>
            <th>BrandName</th>
            <th>BrandName</th>
            <th>Status</th>
            <th>Option</th>
        </tr>
        </thead>
        <tbody class="text-center">
        <% for(let brand of brands) { %>
            <tr>
                <td><%= brand.id %></td>
                <td><%= brand.name %></td>
                <td><%= brand.name %></td>
                <td><%= brand.status %></td>
                <td>
                    <a href="/panel/brands/edit/<%= brand.id %>" class="text-success">Edit</a>
                    <a href="/panel/brands/<%= brand.id %>" class="text-primary">Show</a>
                    <a href="/panel/brands/destroy/<%= brand.id %>" class="text-danger">Delete</a>
                </td>
            </tr>
        <% } %>
        </tbody>
    </table>
</div>
<%- include('../../layouts/panel/footer'); %>


