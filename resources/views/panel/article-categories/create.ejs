<%- include('../../layouts/panel/header'); %>
<div class="container">
    <div class="jumbotron">
        <!--//-->
        <%- include("../../errors/errorMessages.ejs") %>
        <form action="/panel/article-categories/store" method="post">
            <!--//-->
            <div class="row">
                <div class="form-group col-md-6">
                    <input type="text"
                           class="form-control"
                           name="name"
                           id="name"
                           placeholder="Enter Name">
                </div>
                <div class="form-group col-md-6">
                    <input type="radio"
                           class="form-control"
                           name="status"
                           id="status"
                           placeholder="Enter Name">
                </div>
            </div>
            <!--//-->
            <div class="form-group">
                <button class="btn btn-primary">Submit</button>
            </div>
        </form>
    </div>
</div>
<%- include('../../layouts/panel/footer'); %>


